{
    "name": "aerdes/laravel-favicon",
    "description": "Add pretty favicons to your Laravel application on the fly",
    "keywords": [
        "icon",
        "favicon",
        "laravel",
        "generator",
        "tile",
        "apple-touch-icon"
    ],
    "homepage": "https://github.com/aerdes/laravel-favicon",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Lukas MÃ¼ller",
            "email": "lukas@aerdes.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-gd": "*",
        "intervention/image": "^2.4.2",
        "laravel/framework": "^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench-browser-kit": "^4.0|^5.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Aerdes\\LaravelFavicon\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aerdes\\LaravelFavicon\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aerdes\\LaravelFavicon\\FaviconServiceProvider"
            ]
        }
    }
}
